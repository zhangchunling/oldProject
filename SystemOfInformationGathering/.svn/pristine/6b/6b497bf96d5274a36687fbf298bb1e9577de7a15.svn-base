package com.zrgk.email.model;

import java.io.Serializable;

import com.zrgk.utils.Page;

public class Employee extends Page implements Serializable{	
	private static final long serialVersionUID = -2631486271270465972L;
	
	private int eid;			//员工表主键
	private String empName;		//登录名
	private String password;	//密码
	private String realName;	//真实姓名
	private String email;		//邮箱
	private Integer e_state;	//状态
	private String strRole;		//存角色拼接字符串
	private Integer sex;		//性别
	private String tel;			//电话
	private String birthday;	//出生日期
	private String address;		//地址
	
	public String getBirthday() {
		return birthday;
	}
	public void setBirthday(String birthday) {
		this.birthday = birthday;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public Integer getSex() {
		return sex;
	}
	public void setSex(Integer sex) {
		this.sex = sex;
	}
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	public int getEid() {
		return eid;
	}
	public void setEid(int eid) {
		this.eid = eid;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getRealName() {
		return realName;
	}
	public void setRealName(String realName) {
		this.realName = realName;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public Integer getE_state() {
		return e_state;
	}
	public void setE_state(Integer e_state) {
		this.e_state = e_state;
	}
	public String getStrRole() {
		return strRole;
	}
	public void setStrRole(String strRole) {
		this.strRole = strRole;
	}


}
